{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Forevernote (Maven JavaFX)",
            "request": "launch",
            "mainClass": "com.example.forevernote.Main",
            "projectName": "forevernote",
            "preLaunchTask": "maven-exec-java",
            "cwd": "${workspaceFolder}/Forevernote",
            "noDebug": true
        },
        {
            "type": "java",
            "name": "Launch Forevernote (Debug)",
            "request": "launch",
            "mainClass": "com.example.forevernote.Main",
            "projectName": "forevernote",
            "preLaunchTask": "maven-compile",
            "cwd": "${workspaceFolder}/Forevernote",
            "vmArgs": "-Dfile.encoding=UTF-8"
        }
    ]
}
