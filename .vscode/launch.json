{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launcher",
            "request": "launch",
            "mainClass": "com.example.forevernote.Launcher",
            "projectName": "forevernote"
        },
        {
            "type": "java",
            "name": "Run: Main",
            "request": "launch",
            "mainClass": "com.example.forevernote.Main",
            "projectName": "forevernote",
            "cwd": "${workspaceFolder}/Forevernote",
            "vmArgs": [
                "--module-path",
                "${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-controls/21/javafx-controls-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-graphics/21/javafx-graphics-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-base/21/javafx-base-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-fxml/21/javafx-fxml-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-web/21/javafx-web-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-media/21/javafx-media-21-win.jar",
                "--add-modules",
                "javafx.controls,javafx.fxml,javafx.web,javafx.media",
                "-Dfile.encoding=UTF-8"
            ]
        },
        {
            "type": "java",
            "name": "Debug: Main",
            "request": "launch",
            "mainClass": "com.example.forevernote.Main",
            "projectName": "forevernote",
            "cwd": "${workspaceFolder}/Forevernote",
            "vmArgs": [
                "--module-path",
                "${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-controls/21/javafx-controls-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-graphics/21/javafx-graphics-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-base/21/javafx-base-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-fxml/21/javafx-fxml-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-web/21/javafx-web-21-win.jar;${env:USERPROFILE}/.m2/repository/org/openjfx/javafx-media/21/javafx-media-21-win.jar",
                "--add-modules",
                "javafx.controls,javafx.fxml,javafx.web,javafx.media",
                "-Dfile.encoding=UTF-8"
            ]
        },
        {
            "type": "java",
            "name": "Launch via Maven",
            "request": "launch",
            "mainClass": "com.example.forevernote.Main",
            "projectName": "forevernote",
            "preLaunchTask": "maven-javafx-run",
            "cwd": "${workspaceFolder}/Forevernote",
            "noDebug": true
        }
    ]
}